	<!--임기제 공무원 응시자 정보 조회  -->
	<select id="getTermOfficialInfo" parameterType="hashmap" resultType="hashmap">
		<![CDATA[
			SELECT 
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NO
				,NOTIFICATION_NM
				,UUID
				,NAME
				,JUMIN_NO
				,MULTI_NATION_YN
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,TO_CHAR(PAYMENT_DATE, 'YYYY.MM.DD HH24:MI:SS') PAYMENT_DATE
				,PAYMENT_ID
				,PAYMENT
				,PAYMENT_YN
				,APPLY_CLASS
				,TO_CHAR(APPLY_DATE, 'YYYY.MM.DD HH24:MI:SS') APPLY_DATE
				,APPLY_STATE
				,MASTER_NM
				,MASTER_COMMENT
            FROM ILJALI_TERM_OFFICIAL
			WHERE NOT_ANCMT_MGT_NO = #{not_ancmt_mgt_no}
			AND JUMIN_NO = #{jumin_no}
			AND DEL_YN = '0'
		]]>
	</select>
	
	
	<select id="selectTermOfficialList" parameterType="hashmap" resultType="hashmap">
		<![CDATA[
			SELECT 
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NM
				,NOTIFICATION_NO
				,UUID
				,NAME
				,JUMIN_NO
				,MULTI_NATION_YN
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,TO_CHAR(PAYMENT_DATE, 'YYYY.MM.DD HH24:MI:SS') PAYMENT_DATE
				,PAYMENT_ID
				,PAYMENT
				,PAYMENT_YN
				,APPLY_CLASS
				,TO_CHAR(APPLY_DATE, 'YYYY.MM.DD HH24:MI:SS') APPLY_DATE
				,APPLY_STATE
				,MASTER_NM
				,MASTER_COMMENT
            FROM ILJALI_TERM_OFFICIAL
			WHERE 1=1 
		]]>
		<if test="uuid != null and uuid !=''">
			AND UUID = #{uuid}
		</if>
		<if test="apply_state != null and apply_state !=''">
			AND APPLY_STATE = #{apply_state}
			AND TO_CHAR(NVL(UPD_DATE, ''), 'YYYYMMDD') = TO_CHAR(SYSDATE, 'YYYYMMDD')
		</if>
			AND DEL_YN = '0'
	</select>
	

	<!--기간제 근로자 응시자 정보 등록  -->
	<insert id="insertTermWorkerInfo" parameterType="hashmap">
		<![CDATA[
			INSERT INTO ILJALI_TERM_WORKER
			(
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NO
				,NOTIFICATION_NM
				,UUID
				,NAME
				,SEX
				,JUMIN_NO
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,NATIONAL_MERIT_YN
				,DRIVE_YN
				,DRIVE_CLASS_JONG
				,DRIVE_CLASS_GUBUN
				,APPLY_CLASS
				,APPLY_DATE
				,APPLY_STATE
				,INS_DATE
			)
			VALUES
			(			
			    #{buseo_no}
			    ,#{hire_gubun}
			    ,(select NVL(MAX(APPLICATION_SEQ),0)+1 From ILJALI_TERM_OFFICIAL WHERE NOT_ANCMT_MGT_NO = #{not_ancmt_mgt_no})
			    ,#{not_ancmt_mgt_no}
				,#{notification_no}
				,#{notification_nm}
				,#{uuid}
				,#{name}
				,#{sex}
				,#{jumin_no}
				,#{address}
				,#{address_detail}
				,#{post_no}
				,#{phone}
				,#{national_merit_yn}
				,#{drive_yn}
				,#{drive_class_jong}
				,#{drive_class_gubun}
				,#{apply_class}
				,#{apply_date}
				,#{apply_state}
				,sysdate
			)
		]]>
	</insert>
	
	
