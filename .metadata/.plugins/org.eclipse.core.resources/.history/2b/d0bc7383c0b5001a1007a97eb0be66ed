	<!--공공근로사업참여자 정보 조회  -->
	<select id="getPublicBusinessInfo" parameterType="hashmap" resultType="hashmap">
		<![CDATA[
			SELECT 
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NO
				,NOTIFICATION_NM
				,UUID
				,NAME
				,JUMIN_NO
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,HOPE_BUSINESS
				,ETC
				,OBSTACLE
				,OBSTACLE_DEGREE
				,OBSTACLE_CLASS
				,SUPPORT_YN
				,ONE_PARRENT_YN
				,HOMELESS
				,MARRIAGE_MIG_YN
				,NORTH_ESCAPE_YN
				,DBMS_LOB.SUBSTR(LICENSE_INFO, DBMS_LOB.GETLENGTH(LICENSE_INFO)) AS LICENSE_INFO
				,APPLY_CLASS
				,TO_CHAR(APPLY_DATE, 'YYYY.MM.DD HH24:MI:SS') APPLY_DATE
				,APPLY_STATE
				,MASTER_NM
				,MASTER_COMMENT
            FROM ILJALI_PUBLIC_BUSINESS
			WHERE NOT_ANCMT_MGT_NO = #{not_ancmt_mgt_no}
			AND JUMIN_NO = #{jumin_no}
			AND DEL_YN = '0'
		]]>
	</select>
	
	<select id="selectPublicBusinessList" parameterType="hashmap" resultType="hashmap">
		<![CDATA[
			SELECT 
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
