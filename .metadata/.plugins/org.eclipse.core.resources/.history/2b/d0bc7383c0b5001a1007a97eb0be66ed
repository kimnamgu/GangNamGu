	<!--공공근로사업참여자 정보 등록  -->
	<insert id="insertPublicBusinessInfo" parameterType="hashmap">
		<![CDATA[
			INSERT INTO ILJALI_PUBLIC_BUSINESS
			(
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NO
				,NOTIFICATION_NM
				,UUID
				,NAME
				,JUMIN_NO
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,HOPE_BUSINESS
				,ETC
				,OBSTACLE
				,OBSTACLE_DEGREE
				,OBSTACLE_CLASS
				,SUPPORT_YN
				,ONE_PARRENT_YN
				,HOMELESS
				,MARRIAGE_MIG_YN
				,NORTH_ESCAPE_YN
				,LICENSE_INFO
				,APPLY_CLASS
				,APPLY_DATE
				,APPLY_STATE
				,INS_DATE
			)
			VALUES
			(			
			    #{buseo_no}
			    ,#{hire_gubun}
			    ,(select NVL(MAX(APPLICATION_SEQ),0)+1 From ILJALI_TERM_OFFICIAL WHERE NOT_ANCMT_MGT_NO = #{not_ancmt_mgt_no})
			    ,#{not_ancmt_mgt_no}
				,#{notification_no}
				,#{notification_nm}
				,#{uuid}
				,#{name}
				,#{jumin_no}
				,#{address}
				,#{address_detail}
				,#{post_no}
				,#{phone}
				,#{hope_business}
				,#{etc}
				,#{obstacle}
				,#{obstacle_degree}
				,#{obstacle_class}
				,#{support_yn}
				,#{one_parrent_yn}
				,#{homeless}
				,#{marriage_mig_yn}
				,#{north_escape_yn}
				,#{license_info}
				,#{apply_class}
				,#{apply_date}
				,#{apply_state}
				,sysdate
			)
		]]>
	</insert>
	
	
