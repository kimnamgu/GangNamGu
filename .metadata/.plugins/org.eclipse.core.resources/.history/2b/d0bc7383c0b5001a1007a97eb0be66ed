				,POST_NO
				,PHONE
				,HOPE_BUSINESS
				,ETC
				,OBSTACLE
				,OBSTACLE_DEGREE
				,OBSTACLE_CLASS
				,SUPPORT_YN
				,ONE_PARRENT_YN
				,HOMELESS
				,MARRIAGE_MIG_YN
				,NORTH_ESCAPE_YN
				,DBMS_LOB.SUBSTR(LICENSE_INFO, DBMS_LOB.GETLENGTH(LICENSE_INFO)) AS LICENSE_INFO
				,APPLY_CLASS
				,TO_CHAR(APPLY_DATE, 'YYYY.MM.DD HH24:MI:SS') APPLY_DATE
				,APPLY_STATE
				,MASTER_NM
				,MASTER_COMMENT
            FROM ILJALI_PUBLIC_BUSINESS
			WHERE 1=1 
		]]>
		<if test="uuid != null and uuid !=''">
