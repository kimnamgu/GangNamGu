			SELECT 
				BUSEO_NO
				,HIRE_GUBUN
				,APPLICATION_SEQ
				,NOT_ANCMT_MGT_NO
				,NOTIFICATION_NO
				,NOTIFICATION_NM
				,UUID
				,NAME
				,SEX
				,JUMIN_NO
				,ADDRESS
				,ADDRESS_DETAIL
				,POST_NO
				,PHONE
				,NATIONAL_MERIT_YN
				,DRIVE_YN
				,DRIVE_CLASS_JONG
				,DRIVE_CLASS_GUBUN
				,APPLY_CLASS
				,TO_CHAR(APPLY_DATE, 'YYYY.MM.DD HH24:MI:SS') APPLY_DATE
				,APPLY_STATE
				,MASTER_NM
				,MASTER_COMMENT
            FROM ILJALI_TERM_WORKER
			WHERE NOT_ANCMT_MGT_NO = #{not_ancmt_mgt_no}
			AND JUMIN_NO = #{jumin_no}
			AND DEL_YN = '0'
		]]>
	</select>
	
